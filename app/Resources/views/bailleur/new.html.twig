    {% block page_content %}
    {% form_theme form ':widget:fields.html.twig' %}
    <a class="btn red btn-outline sbold" data-toggle="modal" href="#new"> Ajouter Bailleur </a>
    <div class="modal fade" id="new" tabindex="-1" role="basic" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                    <h4 class="modal-title">Creation Bailleur </h4>
                </div>
                {{ form_start(form, {'attr': {'class':'form-horizontal', 'role':'form'}}) }}
                <div class="modal-body">
                        <div class="form-body">
                            {{ form_widget(form) }}
                        </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn dark btn-outline" data-dismiss="modal">Fermer</button>
                    <button type="submit" class="btn blue">Cr√©er</button>
                </div>
                {{ form_end(form) }}
            </div>
            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>
{% endblock %}
